const sortedBundles = this.props.fwaTechRecommenderCBandSOEDigitalFFlag &&
  String(this.props.techRecommendation).toLowerCase() === "high"
  ? cartEligibleBundles.sort((a, b) => {
      const aHasSelf = a.bundleName.includes('Self');
      const bHasSelf = b.bundleName.includes('Self');
      return aHasSelf === bHasSelf ? 0 : aHasSelf ? -1 : 1;
    })
  : cartEligibleBundles.sort((a, b) => {
      const aHasPro = a.bundleName.includes('Pro');
      const bHasPro = b.bundleName.includes('Pro');
      return aHasPro === bHasPro ? 0 : aHasPro ? -1 : 1;
    });
